{
  "type": "module",
  "name": "@kosmojs/api",
  "version": "0.0.20",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pkg/*"
  ],
  "exports": {
    ".": {
      "types": "./pkg/src/index.d.ts",
      "default": "./pkg/index.js"
    },
    "./bodyparser": {
      "types": "./pkg/src/bodyparser/index.d.ts",
      "default": "./pkg/bodyparser/index.js"
    },
    "./queryparser": {
      "types": "./pkg/src/queryparser/index.d.ts",
      "default": "./pkg/queryparser/index.js"
    },
    "./errors": {
      "types": "./pkg/src/errors/index.d.ts",
      "default": "./pkg/errors/index.js"
    }
  },
  "scripts": {
    "build": "esbuilder src/index.ts src/errors/index.ts",
    "test": "vitest --root ../../.. --project core/api"
  },
  "dependencies": {
    "@koa/router": "^15.0.0",
    "formidable": "^3.5.4",
    "koa": "^3.1.1",
    "qs": "^6.14.0",
    "raw-body": "^3.0.2",
    "string-width": "^8.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/formidable": "^3.4.6",
    "@types/koa": "^3.0.1",
    "@types/koa-compose": "^3.2.9",
    "@types/node": "^25.0.3",
    "@types/picomatch": "^4.0.2",
    "@types/qs": "^6.14.0",
    "koa-compose": "^4.1.0"
  }
}
