{
  "type": "module",
  "name": "@kosmojs/cli",
  "version": "0.0.19",
  "author": "Slee Woo",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "pkg/*"
  ],
  "bin": {
    "kosmo": "pkg/cli.js"
  },
  "exports": {
    ".": {
      "types": "./pkg/src/index.d.ts",
      "default": "./pkg/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "esbuilder src/cli.ts src/index.ts",
    "postbuild": "rsync -av --delete src/templates/ pkg/templates",
    "test": "vitest --root ../../.. --project core/cli"
  },
  "dependencies": {
    "@kosmojs/dev": "workspace:^",
    "concurrently": "^9.2.1",
    "prompts": "^2.4.2",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@kosmojs/api": "workspace:^",
    "@solidjs/router": "^0.15.4",
    "@types/koa": "^3.0.1",
    "@types/koa__router": "^12.0.5",
    "@types/node": "^25.0.3",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "esbuild": "^0.27.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.10.1",
    "solid-js": "^1.9.10",
    "tslib": "^2.8.1",
    "typebox": "^1.0.64",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vue": "^3.5.25",
    "vue-router": "^4.6.4"
  }
}
