import type { RouteRecordRaw } from "vue-router";

{{#each routes}}
const {{importName}} = () => import("{{importPathmap.page}}");
{{/each}}

export const routes: Array<RouteRecordRaw> = [
  {{#each routes}}
  {
    name: "{{name}}",
    path: "{{path}}",
    component: {{importName}},
    {{#if meta}}
    info: {{meta}},
    {{/if}}
  },
  {{/each}}
];

export const shouldHydrate = {{shouldHydrate}};
