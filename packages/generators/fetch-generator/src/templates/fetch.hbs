{{#each routes}}
import {{id}} from "{{ createImport "fetch" name }}";
{{/each}}

export { ValidationError } from "@kosmojs/api/errors";

export type PayloadT = {
{{#each routes}}  "{{name}}": import("{{ createImport "fetch" name }}").PayloadT;
{{/each}}
}

export type ResponseT = {
{{#each routes}}  "{{name}}": import("{{ createImport "fetch" name }}").ResponseT;
{{/each}}
}

export default {
{{#each routes}}  "{{name}}": {{id}},
{{#each alias }}  "{{this}}": {{../id}},
{{/each}}
{{/each}}
}
